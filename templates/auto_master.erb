<% if @use_dash_hosts_for_net_bool -%>
/net -hosts
<% else -%>
/net /etc/auto.net --timeout=60
<% end -%>

<% if @maps_real -%>
<%   @maps_real.keys.sort.each do |key| -%>
<%     if @maps_real[key]['mountpoint'] -%>
<%       if @maps_real[key]['maptype'] -%>
/<%= @maps_real[key]['mountpoint'] %><% if @osfamily != 'Solaris' %> <%= @maps_real[key]['maptype'] %><% end %> <%= key -%>
<%       elsif @maps_real[key]['manage'] == false -%>
/<%= @maps_real[key]['mountpoint'] %> -null<% # remove linebreak -%>
<%       else -%>
/<%= @maps_real[key]['mountpoint'] %> /etc/auto.<%= key -%>
<%       end -%>
<%     else -%>
/<%= key %> /etc/auto.<%= key -%>
<%     end -%>
<%     if @maps_real[key]['options'] -%>
 <%= @maps_real[key]['options'] -%>
<%     end %>
<%   end -%>
<% end -%>

<% if @use_nis_maps_bool -%>
+<%= @nis_master_name %>
<% end -%>

/net -hosts

<% if @maps -%>
<% @maps.keys.sort.each do |key| -%>
<% if @maps[key]['mountpoint'] -%>
<% if @maps[key]['maptype'] -%>
/<%= @maps[key]['mountpoint'] -%> <%= @maps[key]['maptype'] -%> <%= key -%>
<% else -%>
/<%= @maps[key]['mountpoint'] %> <% if @maps[key]['manage'] == nil or @maps[key]['manage'] == true -%>/etc/auto.<%= key -%><% else -%>--null<% end -%>
<% end -%>
<% else -%>
/<%= key %> /etc/auto.<%= key -%>
<% end -%>
<% if @maps[key]['options'] %> <%= @maps[key]['options'] %><% end %>
<% end -%>
<% end -%>

<% if @use_nis_maps_real -%>
+<%= @nis_master_name %>
<% end -%>
